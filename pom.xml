    <groupId>com.atlassian.maven</groupId>
    <artifactId>whence-parent</artifactId>
    <version>1.0</version>
    <packaging>pom</packaging>

    <modules>
        <module>whence-test</module>
        <module>whence-maven-plugin</module>
    </modules>

    <scm>
        <connection>scm:git:git@bitbucket.org:atlassian/whence-maven-plugin.git</connection>
        <developerConnection>scm:git:git@bitbucket.org:atlassian/whence-maven-plugin.git</developerConnection>
        <url>https://bitbucket.org/atlassian/whence-maven-plugin</url>
        <tag>whence-parent-1.0</tag>
    </scm>


    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.5.1</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>


</project>